#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,digit;
    scanf("%d",&a);
    int dis=0;
    while(a>0){
        digit=a%10;
        if(digit%2!=0){
            dis+=digit;
            
    }
        a/=10;
    }
    printf("%d",dis);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
