#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a,sum=0;
    while(1)
    {
        scanf("%d",&a);
        if(a==-1)
            break;
        sum=sum+a;
    }
    printf("%d",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
