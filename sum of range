#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int min,max,i,sum=0;
    scanf("%d%d",&min,&max);
    for(i=min;i<=max;i++)
    {
        sum=sum+i;
    }
        printf("%d",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
